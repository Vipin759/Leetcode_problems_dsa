
//888. Fair Candy Swap

class Solution(object):
    def fairCandySwap(self, aliceSizes, bobSizes):
        sum_a = 0
        sum_b = 0
        sum_a = sum(aliceSizes)         
        sum_b = sum(bobSizes)
        set_b = set(bobSizes)
        diff = (sum_a - sum_b) /2
        for x in aliceSizes:
            y = x - diff
            if y in set_b:
                return [x,y]


//136. Single Number
class Solution(object):
    def singleNumber(self, nums):
        result = 0
        for i in nums:
            result ^= i
        return result  
